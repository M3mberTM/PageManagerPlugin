{
  "id": "PageManagerPlugin",
  "name": "Page manager",
  "version": "2.2.0",
  "main": "index.html",
  "manifestVersion": 5,
  "host": [
    {
      "app": "PS",
      "minVersion": "22.0",
      "data": {
        "enableMenuRecording": true
      }
    }
  ],
  "featureFlags": {
    "enableSWCSupport": true,
    "enableAlerts": true
  },
  "requiredPermissions": {
    "localFileSystem": "fullAccess",
    "allowCodeGenerationFromStrings": true,
    "launchProcess": {
      "schemes": [
        "http",
        "https"
      ],
      "extensions": [
        "",
        ".svg",
        ".png"
      ]
    },
    "webview": {
      "allow": "yes",
      "domains": [
        "https://*.adobe.com",
        "https://*.google.com"
      ]
    }
  },
  "entrypoints": [
    {
      "type": "command",
      "id": "showAbout",
      "label": {
        "default": "Plugin info"
      }
    },
    {
      "type": "command",
      "id": "settings",
      "label": {
        "default": "Plugin Settings"
      }
    },
    {
      "type": "panel",
      "id": "import",
      "label": {
        "default": "Import"
      },
      "minimumSize": {
        "width": 245,
        "height": 320
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 250,
        "height": 350
      },
      "preferredFloatingSize": {
        "width": 250,
        "height": 350
      },
      "icons": [
        {
          "width": 32,
          "height": 32,
          "path": "icons/icon_D.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "dark",
            "darkest",
            "all"
          ],
          "species": [
            "generic"
          ]
        },
        {
          "width": 32,
          "height": 32,
          "path": "icons/icon_N.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "species": [
            "generic"
          ]
        }
      ]
    },
    {
      "type": "panel",
      "id": "naming",
      "label": {
        "default": "Naming"
      },
      "minimumSize": {
        "width": 245,
        "height": 350
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 250,
        "height": 350
      },
      "preferredFloatingSize": {
        "width": 250,
        "height": 350
      },
      "icons": [
        {
          "width": 32,
          "height": 32,
          "path": "icons/icon_D.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "dark",
            "darkest"
          ],
          "species": [
            "generic"
          ]
        },
        {
          "width": 32,
          "height": 32,
          "path": "icons/icon_N.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "species": [
            "generic"
          ]
        }
      ]
    },
    {
      "type": "panel",
      "id": "export",
      "label": {
        "default": "Movement"
      },
      "minimumSize": {
        "width": 200,
        "height": 570
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 230,
        "height": 550
      },
      "preferredFloatingSize": {
        "width": 230,
        "height": 550
      },
      "icons": [
        {
          "width": 32,
          "height": 32,
          "path": "icons/icon_D.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "dark",
            "darkest"
          ],
          "species": [
            "generic"
          ]
        },
        {
          "width": 32,
          "height": 32,
          "path": "icons/icon_N.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "species": [
            "generic"
          ]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 32,
      "height": 32,
      "path": "icons/icon_D.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "dark",
        "darkest"
      ],
      "species": [
        "generic"
      ]
    },
    {
      "width": 32,
      "height": 32,
      "path": "icons/icon_N.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "lightest",
        "light"
      ],
      "species": [
        "generic"
      ]
    }
  ]
}